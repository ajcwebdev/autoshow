{
  "name": "autoshow",
  "author": "Anthony Campolo",
  "description": "An example workflow for automatically generating customizable show notes for audio and video files.",
  "keywords": [
    "Claude",
    "ChatGPT",
    "Cohere",
    "Mistral",
    "OctoAI",
    "Whisper",
    "Deepgram",
    "AssemblyAI"
  ],
  "main": "autoshow.js",
  "type": "module",
  "bin": {
    "autoshow": "./autoshow.js"
  },
  "scripts": {
    "autoshow": "node --env-file=.env --no-warnings src/autoshow.js",
    "as": "node --env-file=.env --no-warnings src/autoshow.js",
    "serve": "node --env-file=.env --no-warnings --watch server/index.js",
    "fetch": "node --env-file=.env --no-warnings server/fetch.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.26.0",
    "@deepgram/sdk": "^3.5.1",
    "@google/generative-ai": "^0.17.1",
    "@mistralai/mistralai": "^1.0.2",
    "@octoai/sdk": "^1.5.1",
    "assemblyai": "^4.6.1",
    "cohere-ai": "^7.12.0",
    "commander": "^12.1.0",
    "fast-xml-parser": "^4.4.1",
    "ffmpeg-static": "^5.2.0",
    "file-type": "^19.4.1",
    "inquirer": "^10.2.2",
    "node-llama-cpp": "^3.0.0-beta.44",
    "openai": "^4.55.7"
  }
}
