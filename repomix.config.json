{
  "output": {
    "instructionFilePath": ".github/repomix-instruction.md",
    "filePath": "new-llm.md",
    "style": "markdown",
    "parsableStyle": false,
    "fileSummary": true,
    "directoryStructure": true,
    "removeComments": true,
    "removeEmptyLines": true,
    "compress": false,
    "topFilesLength": 5,
    "showLineNumbers": false,
    "copyToClipboard": false,
    "git": {
      "sortByChanges": true,
      "sortByChangesMaxCommits": 100
    }
  },
  "include": [],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      ".github",
      ".github/setup/",
      ".github/workflows/",
      ".github/*.ts",
      ".github/*.md",
      ".github/FUNDING.yml",
      "content/",
      "docs/",
      "prisma/migrations/",
      "src/images/",
      "src/prompts/sections.ts",
      "src/utils/embeddings/create-embed.ts",
      "src/utils/embeddings/query-embed.ts",
      "src/utils/create-clips.ts",
      "src/utils/dash-documents.ts",
      "test/",
      "web/.astro",
      "web/src/env.d.ts",
      "web/.gitignore",
      "web/.npmrc",
      "web/package.json",
      "web/README.md",
      "web/tsconfig.json",
      "whisper.cpp",
      ".dockerignore",
      ".env",
      ".env.example",
      ".gitignore",
      "LICENSE",
      "new-llm.md",
      "new-*.md",
      "new*.md",
      "package-lock.json",
      "railway.json",
      "README.md",
      "repomix.config.json",
      "tsconfig.json"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
}